{
  "name": "react-light-frame",
  "version": "1.0.0-beta",
  "description": "A code skeleton of React.",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel --open",
    "build": "npm run clean && parcel build --no-cache",
    "build:test": "npm run clean && cross-env NODE_ENV=test parcel build --no-cache",
    "debug-server": "http-server ./dist -o -a localhost --proxy http://localhost:8080?",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "prepare": "husky install",
    "clean": "rimraf ./dist"
  },
  "alias": {
    "@/*": "./src/$1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bruce1i/react-light-frame.git"
  },
  "keywords": [],
  "author": "Bruce(Xun Li)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bruce1i/react-light-frame/issues"
  },
  "homepage": "https://github.com/bruce1i/react-light-frame#readme",
  "dependencies": {
    "@tanstack/react-query": "^4.3.4",
    "@tippyjs/react": "^4.2.6",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "jotai": "^1.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "typescript": "^4.8.3"
  }
}
